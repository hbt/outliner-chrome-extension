{
   "background": {
      "persistent": false,
      "scripts": [ "index.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon32.png",
      "default_title": "Outline"
   },
   "description": "Outline is a free service that removes the clutter from news articles so you can read and annotate without distractions.",
   "homepage_url": "https://outline.com",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "32": "img/icon32.png"
   },
   "manifest_version": 2,
   "name": "Outline: Read & annotate without distractions",
   "permissions": [ "activeTab" ],
   "short_name": "Outline",
   "version": "1.0.6",
   "web_accessible_resources": [ "img/outline.png" ]
}
